<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean name="movieA,movieB" class="com.stackroute.domain.Movie" scope="prototype">
        <constructor-arg name="actor" ref="actor1"></constructor-arg>
    </bean>
    <bean name="movieC,movieD" class="com.stackroute.domain.Movie" scope="prototype">
        <constructor-arg name="actor" ref="actor2"></constructor-arg>
    </bean>
    <bean name="movieE,movieF" class="com.stackroute.domain.Movie" scope="prototype">
        <constructor-arg name="actor" ref="actor3"></constructor-arg>
    </bean>


    <bean id="actor1" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="Ram" type="String"/>
        <constructor-arg name="gender" value="Male" type="String"/>
        <constructor-arg name="age" value="21" type="int"/>
    </bean>
    <bean id="actor2" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="Subi" type="String"/>
        <constructor-arg name="gender" value="Female" type="String"/>
        <constructor-arg name="age" value="22" type="int"/>
    </bean>
    <bean id="actor3" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="Surya" type="String"/>
        <constructor-arg name="gender" value="Male" type="String"/>
        <constructor-arg name="age" value="28" type="int"/>
    </bean>
</beans>